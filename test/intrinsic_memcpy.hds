

fn main(): Void {
    val pass = c"PASS"
    val mut pass_ = c""

     copy(&mut pass_, &pass)

     puts(pass_)

     val f = false
     val mut f_ = true

     copy(&mut f_, &f)

     if f_ {
        puts(c"FAIL")
     }

     val one = 1
     val mut _one = 2

     copy(&mut _one, &one)

     if _one != 1 {
        puts(c"FAIL")
     }

}

fn copy[T](destination: *mut T, source: *T): Void {
    @intrinsic.memcpy(destination, source, size_of[T])
}


extern fn puts(*u8): Void = puts
