import submodule.test_utils as u;

def main(): Void {
    val mut x = 30;
    val x_ptr: *mut CInt = &mut x;
    val x_const_ptr: *CInt = x_ptr;

    *x_ptr = 10;
    u.print_int(*x_ptr);
    u.print_int(*x_const_ptr);
    u.print_int(x);

    val mut box = Box(10);
    val box_ptr = &mut box;
    *box_ptr.value = 40;
    u.print_int(box.value);
    u.print_int(*box_ptr.value);
}

struct Box[T] {
    val mut value: T;
}
