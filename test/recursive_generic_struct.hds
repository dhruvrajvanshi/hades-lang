import libc as c;
import hades.memory as memory;

struct Node[T] {
    val m_data: T;
    val m_next: *mut Node[T];
}

struct List[T] {
    val m_head: *mut Node[T];
}

fn main(): Void {
    val l = List(allocate_new(Node(b"hello", nullptr)));
    c.puts(l.m_head.m_data);
}
fn allocate_new[T](value: T): *mut T {
    val ptr = memory.allocate[T](1);
    *ptr = value;
    return ptr;
}
