import libc as c;

interface Convert[Self, Other] {
    fn convert(self: Self): Other;
}

fn main(): Void {
    c.puts(convert[*cchar, Bool](true));
    c.puts(convert[*cchar, Bool](false));
}

fn convert[U, T](t: T): U where Convert[T, U] {
    return Convert[T, U].convert(t);
}

implementation Convert[Bool, *cchar] {
    fn convert(self: Bool): *cchar {
        if self {
            return c"true";
        } else {
            return c"false";
        }
    }
}
