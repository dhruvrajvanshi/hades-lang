
fn main(): Void {
    val mut x = b"PASS"

    val mut x_mut_ptr: *mut *cchar = x.&mut
    val x_ptr: **cchar = x.&
    puts(x_mut_ptr.*)
    puts(x_ptr.*)

    x.&.print()

}
extension StrExtensions for *cchar {
    fn print(*this): Void {
        puts(this.*)
    }
}

extern fn puts(*cchar): Void = puts
