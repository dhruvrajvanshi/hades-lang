
import submodule.test_utils as u;

def main(): Void {
    val x: union[CInt, Bool] = cast(1);
    val x_int: Int = cast(x);
    val x_bool: Bool = cast(x);
    u.print_int(x_int);
    u.print_bool(x_bool);
}

def cast[T, U](value: T): U {
    val copy = value;
    return *pointer_cast[U](&copy);
}
