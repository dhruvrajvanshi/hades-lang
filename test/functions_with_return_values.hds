extern fn puts(*cchar): Void = puts;

fn id(bytes: *cchar): *cchar {
    return bytes;
}

fn first(first: *cchar, second: *cchar): *cchar {
    return first;
}

fn second(first: *cchar, second: *cchar): *cchar {
    return second;
}

fn main(): Void {
    puts(id(c"id"));
    puts(first(c"first", c"second"));
    puts(second(c"first", c"second"));
}
