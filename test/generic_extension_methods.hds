import libc as c;

fn main(): Void {
    c.puts(Pair(b"first", b"second").first());
    c.puts(Pair(b"first", b"second").second());
}

struct Pair[First, Second] {
    val m_first: First;
    val m_second: Second;
}

extension PairExtensions[First, Second] for Pair[First, Second] {
    fn first(this): First {
        return this.m_first;
    }

    fn second(this): Second {
        return this.m_second;
    }
}
