/// Disabling this test temporarily until we have a better way to
/// promote a pointer of type *mut T to *T automatically using
/// an explicit cast in HIRGen
interface Foo[Self] {
    fn foo(self: Self): Void
}
extern fn puts(*cchar): Void = puts

fn main(): Void {
    val x: *mut usize = nullptr

    foo(x)
}

fn foo[T](value: T): Void where Foo[T] {
    Foo[T].foo(value)
}

implementation Foo[*usize] {
    fn foo(self: *usize): Void {
        puts(c"PASS")
    }
}
