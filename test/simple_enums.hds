enum T {
    X
    Y
    Z
}

extern fn puts(*cchar): Void = puts

fn main(): Void {
    puts(str(T.X))
    puts(str(T.Y))
    puts(str(T.Z))

    puts(match T.X {
        Y -> b"FAIL"
        _ -> b"PASS"
    })
}

fn str(value: T): *cchar {
    return match value {
        X -> b"X"
        Y -> b"Y"
        Z -> b"Z"
    }
}
