enum T {
    X
    Y
    Z
}

extern fn puts(*u8): Void = puts

fn main(): Void {
    puts(str(T.X))
    puts(str(T.Y))
    puts(str(T.Z))

    puts(match T.X {
        Y -> c"FAIL"
        _ -> c"PASS"
    })
}

fn str(value: T): *u8 {
    return match value {
        X -> c"X"
        Y -> c"Y"
        Z -> c"Z"
    }
}
