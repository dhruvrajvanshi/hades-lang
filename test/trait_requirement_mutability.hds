
trait Foo[Self] {
    fn foo(self: Self): Void
}
extern fn puts(*u8): Void = puts

fn main(): Void {
    val x: *mut usize = nullptr

    foo(x)
}

fn foo[T](value: T): Void where Foo[T] {
    Foo[T].foo(value)
}

implementation Foo[*usize] {
    fn foo(self: *usize): Void {
        puts(b"PASS")
    }
}
