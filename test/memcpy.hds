import hades.memory as M
fn main(): Void {
    val pass = c"PASS"
    val mut _pass = c"FAIL"
    M.copy(&mut _pass, &pass, 1)
    puts(_pass)

    val f = false
    val mut _f = true
    M.copy(&mut _f, &f, 1)

    if _f {
        puts(c"FAIL")
    }

    val one: usize = 1
    val mut _one: usize = 2

    M.copy(&mut _one, &one, 1)

    if _one != 1 {
        puts(c"FAIL")
    }
}

extern fn puts(*u8): Void = puts
