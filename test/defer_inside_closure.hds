
fn main(): Void {
  val m = b"DEFER_OUTSIDE_CLOSURE"
  defer puts(m)
  puts(b"BEFORE_APPLY")
  apply || {
    val m = b"DEFER_INSIDE_CLOSURE"
    defer puts(m)
    puts(b"BEFORE_DEFER_INSIDE_CLOSURE")
    return
  }

  puts(b"AFTER_APPLY")
}
extern fn puts(*cchar): Void = puts

fn apply(f: || -> Void): Void {
    f()
}
