struct Foo {
    val value: *cchar
}
extension FooExtensions for Foo {
   fn set_value(*mut this, new_value: *cchar): Void {
      this.value = new_value
   }
}

fn main(): Void {
    val mut foo = Foo(b"FAIL")
    (&mut foo).set_value(b"PASS")

    puts(foo.value)
}
extern fn puts(*cchar): Void = puts
