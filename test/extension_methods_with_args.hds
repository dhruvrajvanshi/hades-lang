import libc as c;

fn main(): Void {
    printb(true._and(false));
    printb(true._or(false));
}

extension BoolExt for Bool {
    fn _and(this, that: Bool): Bool {
        return this and that;
    }

    fn _or(this, that: Bool): Bool {
        return this or that;
    }
}

fn printb(value: Bool): Void {
    if value {
        c.puts(b"true");
    } else {
        c.puts(b"false");
    }
}
