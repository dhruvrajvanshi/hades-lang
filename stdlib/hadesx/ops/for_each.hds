trait ForEach[Self] {
    type Item

    fn for_each(self: *Self, fn: |*Item| -> Void): Void
}

fn for_each[Self](value: *Self, fn: |*ForEach[Self].Item| -> Void): Void where ForEach[Self] {
    ForEach[Self].for_each(value, fn)
}

extension ForEachExtensions[T] for T where ForEach[T] {
    fn for_each(*this, fn: |*ForEach[T].Item| -> Void): Void {
        ForEach[T].for_each(this, fn)
    }
}
